{
  "root": true,
  "plugins": [
    "@typescript-eslint"
  ],
  "env": {
    "es2020": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
    // 'plugin:react/recommended'
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },

  "rules": {
    "react/prop-types": "off"
